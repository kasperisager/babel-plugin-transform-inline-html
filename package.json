{
  "name": "babel-plugin-transform-inline-html",
  "version": "1.0.0",
  "description": "Babel plugin for transforming HTML imports to inline string literals",
  "main": "dist/plugin.js",
  "repository": "https://github.com/kasperisager/babel-plugin-transform-inline-html",
  "author": "Kasper Isager <kasperisager@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "xo && ava",
    "prepare": "babel src -d dist"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "xo": {
    "space": true
  },
  "dependencies": {
    "html-minifier": "^3.5.5",
    "micromatch": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "xo": "^0.18.2"
  }
}
